{
  "name": "obuild",
  "version": "0.2.1",
  "description": "Zero-config ESM/TS package builder",
  "repository": "unjs/obuild",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./config": "./dist/config.mjs"
  },
  "types": "./dist/index.d.mts",
  "bin": "./dist/cli.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm obuild",
    "dev": "pnpm vitest",
    "lint": "eslint . && prettier -c src test",
    "lint:fix": "automd && eslint . --fix && prettier -w src test",
    "node-ts": "node --disable-warning=ExperimentalWarning --experimental-strip-types",
    "obuild": "pnpm node-ts src/cli.ts",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && pnpm vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "pnpm": {
    "overrides": {
      "@obuild/plugin": "file:plugins/obuild-plugin-0.0.1.tgz",
      "@obuild/plugin-oxc-transform": "file:plugins/obuild-plugin-oxc-transform-0.0.1.tgz",
      "@obuild/plugin-oxc-dts": "file:plugins/obuild-plugin-oxc-dts-0.0.1.tgz",
      "@obuild/plugin-oxc-minify": "file:plugins/obuild-plugin-oxc-minify-0.0.1.tgz",
      "@obuild/plugin-postcss": "file:plugins/obuild-plugin-postcss-0.0.1.tgz",
      "@obuild/plugin-sass": "file:plugins/obuild-plugin-sass-0.0.1.tgz",
      "@obuild/plugin-shared": "file:plugins/obuild-plugin-shared-0.0.1.tgz",
      "@obuild/plugin-tsc-dts": "file:plugins/obuild-plugin-tsc-dts-0.0.1.tgz",
      "@obuild/plugin-tsgo-dts": "file:plugins/obuild-plugin-tsgo-dts-0.0.1.tgz",
      "@obuild/plugin-vue-tsc-dts": "file:plugins/obuild-plugin-vue-tsc-dts-0.0.1.tgz",
      "@obuild/plugin-vue-sfc-transformer": "file:plugins/obuild-plugin-vue-sfc-transformer-0.0.1.tgz"
    }
  },
  "dependencies": {
    "@obuild/plugin": "file:plugins/obuild-plugin-0.0.1.tgz",
    "@obuild/plugin-oxc-transform": "file:plugins/obuild-plugin-oxc-transform-0.0.1.tgz",
    "@obuild/plugin-oxc-dts": "file:plugins/obuild-plugin-oxc-dts-0.0.1.tgz",
    "@obuild/plugin-oxc-minify": "file:plugins/obuild-plugin-oxc-minify-0.0.1.tgz",
    "@obuild/plugin-postcss": "file:plugins/obuild-plugin-postcss-0.0.1.tgz",
    "@obuild/plugin-sass": "file:plugins/obuild-plugin-sass-0.0.1.tgz",
    "@obuild/plugin-shared": "file:plugins/obuild-plugin-shared-0.0.1.tgz",
    "@obuild/plugin-tsc-dts": "file:plugins/obuild-plugin-tsc-dts-0.0.1.tgz",
    "@obuild/plugin-tsgo-dts": "file:plugins/obuild-plugin-tsgo-dts-0.0.1.tgz",
    "@obuild/plugin-vue-tsc-dts": "file:plugins/obuild-plugin-vue-tsc-dts-0.0.1.tgz",
    "@obuild/plugin-vue-sfc-transformer": "file:plugins/obuild-plugin-vue-sfc-transformer-0.0.1.tgz",
    "autoprefixer": "^10.4.21",
    "c12": "^3.0.4",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "exsolve": "^1.0.5",
    "oxc-minify": "^0.72.3",
    "oxc-parser": "^0.72.3",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "pretty-bytes": "^7.0.0",
    "rolldown": "1.0.0-beta.17",
    "rolldown-plugin-dts": "^0.13.11",
    "tinyglobby": "^0.2.14"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@vitest/coverage-v8": "^3.2.2",
    "automd": "^0.4.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.28.0",
    "eslint-config-unjs": "^0.4.2",
    "get-tsconfig": "^4.10.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  },
  "packageManager": "pnpm@10.11.1"
}
